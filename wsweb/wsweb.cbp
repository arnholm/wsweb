<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="wsweb" />
		<Option pch_mode="2" />
		<Option compiler="gcc_generic" />
		<Build>
			<Target title="MSVC_Debug">
				<Option output=".cmp/msvc/bin/Debug/wswebd" prefix_auto="0" extension_auto="1" />
				<Option object_output=".cmp/msvc/obj/Debug/" />
				<Option type="1" />
				<Option compiler="msvc" />
				<Compiler>
					<Add option="/MDd" />
					<Add option="/GF" />
					<Add option="/Od" />
					<Add option="/W3" />
					<Add option="/RTCsu" />
					<Add option="/Fd$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).pdb" />
					<Add option="/EHsc" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/D_CRT_NONSTDC_NO_DEPRECATE" />
					<Add option="/D_CRT_SECURE_DEPRECATE" />
					<Add option="/D_WIN32_WINNT=0x0601" />
				</Compiler>
				<Linker>
					<Add option="/NODEFAULTLIB:libcmt.lib" />
					<Add option="/NODEFAULTLIB:msvcrt.lib" />
					<Add option="/INCREMENTAL:NO" />
					<Add library="msvcrtd.lib" />
					<Add library="wsapid" />
				</Linker>
			</Target>
			<Target title="MSVC_Release">
				<Option output=".cmp/msvc/bin/Release/wsweb" prefix_auto="0" extension_auto="1" />
				<Option object_output=".cmp/msvc/obj/Release/" />
				<Option type="1" />
				<Option compiler="msvc" />
				<Compiler>
					<Add option="/MD" />
					<Add option="/GF" />
					<Add option="/Ox" />
					<Add option="/W3" />
					<Add option="/EHsc" />
					<Add option="/D_CRT_SECURE_NO_WARNINGS" />
					<Add option="/D_CRT_NONSTDC_NO_DEPRECATE" />
					<Add option="/D_CRT_SECURE_DEPRECATE" />
					<Add option="/D_WIN32_WINNT=0x0601" />
				</Compiler>
				<Linker>
					<Add option="/NODEFAULTLIB:libcmtd.lib" />
					<Add option="/NODEFAULTLIB:msvcrtd.lib" />
					<Add option="/INCREMENTAL:NO" />
					<Add library="msvcrt.lib" />
					<Add library="wsapi" />
				</Linker>
				<ExtraCommands>
					<Add after="$(CPDE_USR)/bin/cpde_usr -bin -xi -project=$(PROJECT_NAME)  -root=$(PROJECT_DIR)  -build=$(TARGET_NAME)  -target=$(TARGET_OUTPUT_FILE)  -usr=$(CPDE_USR)" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="GCC_Debug">
				<Option output=".cmp/gcc/bin/Debug/wswebd" prefix_auto="1" extension_auto="1" />
				<Option object_output=".cmp/gcc/obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc_generic" />
				<Compiler>
					<Add option="-std=c++17" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-fexceptions" />
					<Add option="-g" />
					<Add option="-DNOPCH" />
					<Add option="-D_DEBUG" />
					<Add directory="./" />
				</Compiler>
				<Linker>
					<Add library="pthread" />
					<Add library="wsapid" />
				</Linker>
			</Target>
			<Target title="GCC_Release">
				<Option output=".cmp/gcc/bin/Release/wsweb" prefix_auto="1" extension_auto="1" />
				<Option object_output=".cmp/gcc/obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc_generic" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-std=c++17" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-fexceptions" />
					<Add option="-Os" />
					<Add option="`$(#wx.config) --cxxflags`" />
					<Add option="-DNOPCH" />
					<Add directory="./" />
				</Compiler>
				<Linker>
					<Add library="pthread" />
					<Add library="wsapi" />
				</Linker>
				<ExtraCommands>
					<Add before="rm -f $(TARGET_OUTPUT_FILE)" />
					<Add before="$(CPDE_USR)/bin/txt2h ./templates/index.html" />
					<Add after="$(CPDE_USR)/bin/cpde_usr -bin -xi -project=$(PROJECT_NAME)  -root=$(PROJECT_DIR)  -build=$(TARGET_NAME)  -target=$(TARGET_OUTPUT_FILE)  -usr=$(CPDE_USR)" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add directory="$(CPDE_USR)/include" />
			<Add directory="$(#boost.include)" />
			<Add directory="$(#crow.include)" />
			<Add directory="$(#dotenv.include)" />
		</Compiler>
		<Linker>
			<Add directory="$(CPDE_USR)/lib" />
			<Add directory="$(#boost.lib)" />
			<Add directory="$(#dotenv.lib)" />
		</Linker>
		<Unit filename="main.cpp" />
		<Unit filename="multipart_params.h" />
		<Unit filename="templates/index.html" />
		<Unit filename="wsapi.env" />
		<Unit filename="wsapi_server.cpp" />
		<Unit filename="wsapi_server.h" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
